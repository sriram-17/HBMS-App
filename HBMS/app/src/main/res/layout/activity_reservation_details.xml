<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ReservationDetails">

  <TextView
      android:id="@+id/text3"
      android:layout_width="wrap_content"
      android:layout_height="34dp"
      android:text="Name"
      android:textSize="21sp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.084"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.282" />

  <TextView
      android:id="@+id/text4"
      android:layout_width="wrap_content"
      android:layout_height="34dp"
      android:text="Age"
      android:textSize="21sp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.084"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.351" />

  <TextView
      android:id="@+id/text5"
      android:layout_width="wrap_content"
      android:layout_height="34dp"
      android:text="Email"
      android:textSize="21sp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.084"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.42" />

  <TextView
      android:id="@+id/text6"
      android:layout_width="wrap_content"
      android:layout_height="34dp"
      android:text="Phone"
      android:textSize="21sp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.07"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.489" />

  <TextView
      android:id="@+id/text7"
      android:layout_width="wrap_content"
      android:layout_height="34dp"
      android:text="Bed Type"
      android:textSize="21sp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.076"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.558" />

  <TextView
      android:id="@+id/text8"
      android:layout_width="wrap_content"
      android:layout_height="34dp"
      android:text="Symptoms"
      android:textSize="21sp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.08"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.634" />

  <TextView
      android:id="@+id/text9"
      android:layout_width="wrap_content"
      android:layout_height="34dp"
      android:text="Covid Report"
      android:textSize="21sp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.085"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.724" />

  <TextView
      android:id="@+id/pName"
      android:layout_width="227dp"
      android:layout_height="34dp"
      android:text="Patient Name"
      android:textSize="21sp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.836"
      app:layout_constraintStart_toEndOf="@+id/text3"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.282" />

  <TextView
      android:id="@+id/pAge"
      android:layout_width="227dp"
      android:layout_height="34dp"
      android:text="Patient Age"
      android:textSize="21sp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.894"
      app:layout_constraintStart_toStartOf="@+id/text4"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.351" />

  <TextView
      android:id="@+id/pEmail"
      android:layout_width="227dp"
      android:layout_height="34dp"
      android:text="Patient Email"
      android:textSize="21sp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.844"
      app:layout_constraintStart_toEndOf="@+id/text5"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.42" />

  <TextView
      android:id="@+id/pPhone"
      android:layout_width="227dp"
      android:layout_height="34dp"
      android:text="Patient Phone"
      android:textSize="21sp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.84"
      app:layout_constraintStart_toEndOf="@+id/text6"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.489" />

  <TextView
      android:id="@+id/pBedtype"
      android:layout_width="227dp"
      android:layout_height="34dp"
      android:text="Bed Type"
      android:textSize="21sp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.78"
      app:layout_constraintStart_toEndOf="@+id/text7"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.558" />

  <TextView
      android:id="@+id/pSymptoms"
      android:layout_width="227dp"
      android:layout_height="34dp"
      android:text="Patient Symptoms"
      android:textSize="21sp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.728"
      app:layout_constraintStart_toEndOf="@+id/text8"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.634" />

  <Button
      android:id="@+id/pReport"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:text="Covid Report"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.401"
      app:layout_constraintStart_toEndOf="@+id/text9"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.739" />

  <Button
      android:id="@+id/accept"
      android:layout_width="125dp"
      android:layout_height="50dp"
      android:layout_marginBottom="100dp"
      android:drawableLeft="@drawable/ic_done"
      android:text="Accept"
      app:backgroundTint="#188118"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.15"
      app:layout_constraintStart_toStartOf="parent" />

  <Button
      android:id="@+id/cancel2"
      android:layout_width="125dp"
      android:layout_height="50dp"
      android:layout_marginBottom="100dp"
      android:backgroundTint="@color/red"
      android:drawableLeft="@drawable/ic_cancel"
      android:text="Cancel"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.38"
      app:layout_constraintStart_toEndOf="@+id/accept" />



</androidx.constraintlayout.widget.ConstraintLayout>